{
  "name": "docusaurus-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "docusaurus start --port 1919",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "serve": "docusaurus serve --port 1919",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "diagrams:generate": "puml generate -s docs/diagrams/current-workflow.puml -o docs/diagrams/current-workflow.svg && puml generate -s docs/diagrams/automated-workflow.puml -o docs/diagrams/automated-workflow.svg && puml generate -s docs/diagrams/data-flow.puml -o docs/diagrams/data-flow.svg",
    "diagrams:current": "puml generate -s docs/diagrams/current-workflow.puml -o docs/diagrams/current-workflow.svg",
    "diagrams:automated": "puml generate -s docs/diagrams/automated-workflow.puml -o docs/diagrams/automated-workflow.svg",
    "diagrams:dataflow": "puml generate -s docs/diagrams/data-flow.puml -o docs/diagrams/data-flow.svg"
  },
  "dependencies": {
    "@docusaurus/core": "3.9.1",
    "@docusaurus/preset-classic": "3.9.1",
    "@docusaurus/theme-mermaid": "^3.9.1",
    "@mdx-js/react": "^3.0.0",
    "@mstroppel/remark-local-plantuml": "^0.0.1",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rehype-raw": "^7.0.0"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "3.9.1",
    "@docusaurus/tsconfig": "3.9.1",
    "@docusaurus/types": "3.9.1",
    "prettier": "^3.3.3",
    "typescript": "~5.6.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=20.0"
  }
}
